pipeline{
	agent{ 
		dockerfile{
    		additionalBuildArgs '-t webapp-docker'
		}

	}
	stages{
		stage('build mvn'){
			steps {
			
			
				
			   	sh 'mvn clean verify' 
			   	
			   
			}

    
		}
		
		
		post{
			always{
				sh "docker image rm webapp-docker" 
			}            
		}


	   
	}

}
